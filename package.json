{
    "name": "react-starter",
    "version": "1.2.0",
    "description": "⚛️ react starter",
    "type": "module",
    "keywords": [
        "typescript",
        "eslint",
        "prettier",
        "vitest",
        "react",
        "hacktoberfest",
        "vercel",
        "tailwind"
    ],
    "homepage": "https://bradgarropy.com",
    "bugs": {
        "url": "https://github.com/bradgarropy/react-starter/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Brad Garropy",
        "email": "bradgarropy@gmail.com",
        "url": "https://bradgarropy.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/bradgarropy/react-starter"
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "lint": "eslint .",
        "lint:fix": "eslint --fix .",
        "format": "prettier --check .",
        "format:fix": "prettier --write .",
        "typecheck": "tsc",
        "test": "vitest run --coverage",
        "test:watch": "vitest",
        "contributors": "all-contributors generate"
    },
    "devDependencies": {
        "@bradgarropy/eslint-config": "^1.2.3",
        "@bradgarropy/eslint-config-typescript": "^1.2.0",
        "@types/react": "^18.3.2",
        "@types/react-dom": "^18.3.0",
        "@typescript-eslint/eslint-plugin": "^7.9.0",
        "@typescript-eslint/parser": "^7.9.0",
        "@vitejs/plugin-react": "^4.2.1",
        "@vitejs/plugin-react-swc": "^3.6.0",
        "@vitest/coverage-v8": "^3.1.2",
        "all-contributors-cli": "^6.26.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^8.10.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "jsdom": "^26.1.0",
        "prettier": "^3.1.1",
        "typescript": "^5.3.3",
        "vite": "^6.3.3",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^3.1.2"
    },
    "dependencies": {
        "@tailwindcss/vite": "^4.1.4",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwindcss": "^4.1.4"
    }
}
